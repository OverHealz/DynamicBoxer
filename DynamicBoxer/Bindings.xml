<Bindings>
    <Binding name="DBOX_INVITE" header="DYNAMICBOXER" category="DYNAMICBOXER" default="CTRL-SHIFT-I">
        DynBoxer.Slash("party invite")
    </Binding>
    <Binding name="DBOX_DISBAND" category="DYNAMICBOXER" default="CTRL-SHIFT-U">
        DynBoxer.Slash("party disband")
    </Binding>
    <Binding name="DBOX_XCHG" category="DYNAMICBOXER" default="CTRL-SHIFT-X">
        DynBoxer.Slash("xchg")
    </Binding>
    <Binding name="DBOX_SHOW" category="DYNAMICBOXER">
        DynBoxer.Slash("show")
    </Binding>
    <Binding name="DBOX_PING" category="DYNAMICBOXER">
        DynBoxer.Slash("message")
    </Binding>
    <Binding name="DBOX_JOIN" category="DYNAMICBOXER">
        DynBoxer.Slash("join")
    </Binding>
    <Binding name="DBOX_AUTOINV" category="DYNAMICBOXER" default="CTRL-SHIFT-T">
        DynBoxer.Slash("autoinvite toggle")
    </Binding>
    <Binding name="DBOX_CONFIG" category="DYNAMICBOXER">
        DynBoxer.Slash("config")
    </Binding>
</Bindings>
